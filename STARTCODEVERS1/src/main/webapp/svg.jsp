       xmlns:xlink="http://www.w3.org/1999/xlink"
       x="75"
       y="10"
       width="800"
       height="600"
       viewBox="0 0 780 600"
       preserveAspectRatio="xMinYMin">
    <rect x="0" y="0" height="560" width="760"
          style="stroke:#000000; fill: #ffffff"/>
    <line x1="10" x2="10" y1="0" y2="560" stroke="black"></line>
    <line x1="750" x2="750" y1="0" y2="560" stroke="black"></line>
    <line x1="10" x2="750" y1="40" y2="40" stroke="black"></line>
    <line x1="10" x2="750" y1="520" y2="520" stroke="black"></line>
